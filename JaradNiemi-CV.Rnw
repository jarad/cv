\section{\bf Student Committees and Advisees}

\vspace{0.1in}

<<read_studentcommittees, cache.extra = tools::md5sum("studentcommittees.csv")>>=
# library(plyr)
library(dplyr)
library(xtable)

d = readr::read_csv("studentcommittees.csv") %>%
  mutate(School = factor(School, levels=c("ISU","UCSB")),
         Chair  = factor(Chair,  levels=c("Chair","Co-chair","")))

d_levels = unique(d$Department)
d$Department = factor(d$Department, 
                      levels = c("STAT",
                                 d_levels[-which(d_levels=="STAT")]))

d = d %>% 
  arrange(School, desc(Completed), Degree, Chair, Department)
@


<<advisees, dependson="read_studentcommittees", results='asis'>>=
tab = xtable(d %>% 
               filter(Chair %in% c('Chair','Co-chair')) %>%
               arrange(School, Degree, desc(Completed), Chair), 
             caption = "Students advised",
             label   = "tab:advisees")

print(tab,
      include.rownames  = FALSE,
      table.placement   = "h",
      caption.placement = "top")
@


<<committees, dependson="read_studentcommittees", results='asis'>>=
s = d %>%
  # filter(!(Chair %in% c("Chair","Co-chair"))) %>%
  mutate(`Chair/Co-chair` = ifelse(Chair %in% c("Chair","Co-chair"), 'Yes', "No"),
         STAT = ifelse(Department %in% c('STAT','PSTAT','STAT&ECON'), 'Yes', 'No'),
         prog = ifelse(Completed == 'In progress', 'In progress', 'Completed')) %>%
  group_by(Degree, `Chair/Co-chair`, STAT, prog) %>%
  dplyr::summarize(n = length(Student)) %>%
  tidyr::spread(prog,n,fill=0) %>%
  select(`Chair/Co-chair`, STAT, Degree, Completed, `In progress`) %>%
  arrange(desc(`Chair/Co-chair`), STAT, Degree)

tab2 = xtable(s, 
              caption='All student committees',
              label="tab:committees",
              digits=0)

print(tab2, 
      include.rownames=FALSE,
      table.placement="h",
      caption.placement="top")
@



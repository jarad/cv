\subsection{\bf Refereed Journal Articles} \vspace{-0.2in}

<<published, dependson="read_pubs", results='asis'>>=
published_print = function(d) {
  in_print = !is.na(d$volume)
  
  if(!in_print) {
    end = ""
  } else {
    end = paste0("{ \\bf ", d$volume, "}")
  }
  
  if (!is.na(d$issue))  end = paste0(end,"(",d$issue,")")
  if (!is.na(d$number)) end = paste0(end,":",d$number)
  if (!is.na(d$pages))  end = paste0(end, ", ", d$pages)
  if (in_print)         end = paste0(end,". ")
  
  cat(d$authors, ". ",
      "(",d$year,") ",
      "``",d$title, '"',
      ifelse(in_print, 
             paste0("\\emph{",d$journal,"}, "), 
             paste0("\\emph{ to appear in ", d$journal, "} ")),
      end,
      ifelse(is.na(d$url), "", paste0("(\\href{",d$url,"}{url}) ")),
      "\n\n",
      sep="")
  return(data.frame(a=1)) # to avoid error
}


tmp = pubs %>% 
  filter(status=="published", new) %>%
  arrange(-year) %>%
  rowwise() %>%
  do(published_print(.))

cat("\n\n\\sinceappt\n\n")

tmp = pubs %>% 
  filter(status=="published", !new) %>%
  arrange(-year) %>%
  rowwise() %>%
  do(published_print(.))
@


